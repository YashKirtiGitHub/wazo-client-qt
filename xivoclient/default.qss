QLineEdit {
height: 30px;
border-radius: 15px; 
padding-left: 15px;
padding-right: 15px;
background-color: #D7D2D0;
font: 10pt "Liberation Sans";
color: #2C2927;
}

QPushButton{
height: 30px;
border-radius: 8px; 
padding-left: 18px;
padding-right: 18px;
}
QPushButton[qss_tags~="orange"]{
background-color: #E77D39;
font: 10pt "Dyno";
color: #FFFFFF;
}
QPushButton[qss_tags~="black"]{
background-color: #58524F;
font: 10pt "Dyno";
color: #FFFFFF;
padding-left: 22px;
padding-right: 22px;
}

QFrame[qss_tags~="input_button"]{
margin-top: 15px;
margin-bottom: 15px;
border-radius: 15px; 
background-color: #D7D2D0;
}

.QWidget[qss_tags~="xlet_frame"] {
/*margin: 30px; /*unused*/
background-color: #FFFFFF;
}

QRadioButton{
margin-top: 15px;
margin-bottom: 15px;
}
QCheckBox{
margin-top: 8px;
margin-bottom: 8px;
}

QLabel[qss_tags~="xlet_h1"] {
padding-bottom: 30px;
font: bold 26pt "Dyno";
color: #E77D39;
}
QLabel[qss_tags~="xlet_h2"] {
padding-bottom: 30px;
font: bold 22pt "Dyno";
color: #E77D39;
}
*[qss_tags~=xlet_list]{
font: 11pt "Liberation Sans";
}
*[qss_tags~=xlet_sub_list]{
font: 9pt "Liberation Sans";
}

/* 
  * Fax Xlet 
  */
QLabel[qss_tags~="step_number"] {
font: bold 26pt "Dyno";
color: #D7D2D0;
}

