QLineEdit {
height: 30px;
border-radius: 15px; 
padding-left: 15px;
background-color: #D7D2D0;
font: 10pt "Liberation Sans";
color: #2C2927;
}
QPushButton{
height: 30px;
border-radius: 8px; 
padding-left: 18px;
padding-right: 18px;
}
QPushButton[qss_tags~="orange"]{
background-color: #E77D39;
font: 10pt "Dyno";
color: #FFFFFF;
}
QPushButton[qss_tags~="black"]{
background-color: #58524F;
font: 10pt "Dyno";
color: #FFFFFF;
padding-left: 22px;
padding-right: 22px;
}
QFrame[qss_tags~="line_edit_button"]{
margin-top: 15px;
margin-bottom: 15px;
border-radius: 15px; 
background-color: #D7D2D0;
}
QLabel[qss_tags~="xlet_title"] {
padding-bottom: 30px;
font: bold 26pt "Dyno";
color: #E77D39;
}
QLabel[qss_tags~="xlet_subtitle"] {
font: 13pt "Dyno";
}
QWidget[qss_tags~="xlet_frame"] {
/*margin: 30px; /*unused*/
background-color: #FFFFFF;
}
/* 
